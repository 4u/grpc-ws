{
  "name": "grpc-ws",
  "version": "0.3.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cleanup": "rm -rf lib && mkdir lib",
    "pb": "npm run pb-js & npm run pb-ts",
    "pb-js": "pbjs -t json-module -w commonjs -o src/grpc-bus.js node_modules/grpc-bus/proto/grpc-bus.proto",
    "pb-ts": "pbjs -t static-module node_modules/grpc-bus/proto/grpc-bus.proto | pbts -o src/grpc-bus.d.ts -",
    "build": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "grpc-bus": "4u/grpc-bus#56d35479d5e956ac6eb10a6d0a9b4ed668366611",
    "universal-websocket-client": "1.0.2"
  },
  "peerDependencies": {
    "grpc": ">= 1.24.4 < 2",
    "protobufjs": ">= 6.10.2 < 7"
  },
  "devDependencies": {
    "@types/ws": "7.4.0",
    "chalk": "^4.1.0",
    "escodegen": "^2.0.0",
    "espree": "^7.3.1",
    "estraverse": "^5.2.0",
    "jsdoc": "^3.6.6",
    "semver": "^7.3.4",
    "tmp": "^0.2.1",
    "ts-proto": "1.55.0",
    "tslib": "2.1.0",
    "typescript": "4.1.4",
    "uglify-js": "^3.12.7"
  }
}
